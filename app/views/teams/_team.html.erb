<div class="team_detailed_view">

<%= render :partial => "teams/header", :locals => {:team => team} %>

<p class="team_member_list">
  <b>Members:</b>
  <ul>
  <% team.members.sort_by{|x| x.last_name}.each do |member| %>
    <li>
    <%= member.lastname_firstname %>
    </li>
  <% end %>
  </ul>
</p>
</div>
